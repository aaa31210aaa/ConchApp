        <activity
            android:name="tab.minedetail.ModifyPwd"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity android:name="tab.homedetail.Ledger" />
        <activity android:name="tab.homedetail.ledger.TruckLedger" />
        <activity android:name="tab.homedetail.ledger.ForkliftLedger" />
        <activity android:name="tab.homedetail.ledger.BlastLedger" />
        <activity android:name="tab.homedetail.ledger.TransitionLedger" />
