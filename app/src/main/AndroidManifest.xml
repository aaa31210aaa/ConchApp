            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.blastholedetail.BlastholeDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.formdetail.FormDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.formdetail.SbyxFormDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.formdetail.XhtjFormDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.homedetail.earlywarningdetail.AlarmDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="tab.prodetail.KcjhDetail"
            android:configChanges="keyboardHidden|orientation|screenSize"
