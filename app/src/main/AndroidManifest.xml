        <activity android:name="tab.homedetail.ledger.TruckLedgerDetail" />
        <activity android:name="tab.homedetail.ledger.AuxiliaryLedger" />
        <activity android:name="tab.homedetail.ledger.AddTruckLedger" />
        <activity android:name="tab.homedetail.ledger.AddTruckWork" />
        <activity android:name="tab.homedetail.ledger.TransLedger"></activity>
    </application>

</manifest>